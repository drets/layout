default partial alphanumeric_keys modifier_keys
xkb_symbols "neongreen" {

    name[Group1]= "English (neongreen)";

    include "neongreen(pre)"
    include "us(basic)"
    include "neongreen(post)"

    # J and D are temporarily moved to Backspace
    key <BKSP> {[ j, J, d, D ]};

    # S is temporarily moved to 4
    key <AE04> {[ 4, dollar, s, S ]};
};
